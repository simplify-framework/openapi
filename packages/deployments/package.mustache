{
    "name": "{{projectNameSnake}}",
    "version": "{{projectVersion}}",
    "description": "{{{projectDescription}}}",
    "scripts": {
        "test": "cd {{projectName}} && npm install && npm run test",
        "test": "cd {{projectName}} && npm install && npm run test-units",
        "test": "cd {{projectName}} && npm install && npm run test-apis",
        "start": "npm install && npm run stack-deploy && cd {{projectName}} && npm install && npm run test-apis",
        "gencode": "simplify-codegen generate -i openapi.yaml -o . --merge",
        "stack-deploy": "node deployment.js --input {{deploymentNameSnake}}.json",
        "stack-destroy": "node deployment.js --input {{deploymentNameSnake}}.json --deletion",
        "push-update": "bash code-push.sh .env update",
        "push-code": "bash code-push.sh .env push"
    },
    "license": "MIT",
    "private": true,
    "devDependencies": {
        "simplify-sdk": "^0.1.10"
    }
}
